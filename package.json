{
  "name": "napcat-plugin-group-approval",
  "plugin": "加群申请审批系统",
  "version": "1.0.0",
  "type": "module",
  "main": "index.mjs",
  "description": "根据正则表达式自动审批群入申请，支持按群设置不同规则",
  "author": "zxxhz",
  "license": "MIT",
  "keywords": [
    "napcat",
    "plugin",
    "qq",
    "group",
    "approval",
    "regex"
  ],
  "napcat": {
    "tags": [
      "管理工具",
      "自动化"
    ],
    "minVersion": "4.14.0",
    "homepage": "https://github.com/zxxhz/napcat-plugin-group-approval"
  },
  "dependencies": {
    "napcat-types": "0.0.16"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@types/express": "^5.0.6",
    "@types/node": "^20.19.33",
    "napcat-plugin-debug-cli": "^1.2.8",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "ws": "^8.19.0"
  },
  "scripts": {
    "build": "vite build",
    "build:webui": "cd src/webui && pnpm install && pnpm run build",
    "dev:webui": "cd src/webui && pnpm run dev",
    "watch": "vite build --watch",
    "dev": "vite build --watch",
    "push": "vite build",
    "deploy": "vite build",
    "typecheck": "tsc --noEmit"
  }
}